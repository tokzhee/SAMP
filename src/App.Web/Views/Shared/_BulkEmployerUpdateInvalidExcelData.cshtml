<p>
    <table class="table table-striped table-hover table-responsive table-sm ">
        <thead>
            <tr>
                <th>#</th>
                <th>Account Number</th>
                <th>Employer Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in (List<App.Web.Models.BulkEmployerUpdateExcelData>)TempData["BulkEmployerUpdateInvalidExcelData"])
            {
                <tr>
                    <td>@item.Sn</td>
                    <td>@item.AccountNumber</td>
                    <td>@item.EmployerName</td>
                </tr>            
            }
        </tbody>
    </table>
</p>

